.DS_Store
config/private.yml
config/private.xsl
/logs/*

# scripts/.xslt-datura is populated by the datura gem and should not be tracked / edited
/scripts/.xslt-datura/*

# ignore all output types except for production html and iiif
/output/*
!/output/production
/output/production/*
!/output/production/html
!/output/production/iiif

# including dev output
!/output/development
/output/development/*
!/output/development/html
!/output/development/iiif
